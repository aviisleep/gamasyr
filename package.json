{
  "name": "new-gama",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "build:production": "vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "optimize-images": "node scripts/optimize-images.cjs",
    "optimize-images:check": "node scripts/optimize-images.cjs --check-only",
    "build:optimized": "npm run optimize-images && npm run build:production",
    "analyze": "npm run build:analyze && npx vite-bundle-analyzer dist/assets",
    "performance": "npm run build:production && npx lighthouse http://localhost:4173 --output=html --output-path=./lighthouse-report.html",
    "audit": "npm audit && npm run lint",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "clean": "rm -rf dist node_modules/.vite",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npm run optimize-images:check"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "emailjs-com": "^3.2.0",
    "framer-motion": "^10.16.4",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.4",
    "react-icons": "^4.11.0",
    "react-intersection-observer": "^9.16.0",
    "react-lazy-load-image-component": "^1.6.0",
    "react-router-dom": "^6.8.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "lighthouse": "^11.0.0",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.3",
    "terser": "^5.19.0",
    "vite": "^4.5.14",
    "vite-bundle-analyzer": "^0.7.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "server": {
    "compress": true
  }
}
